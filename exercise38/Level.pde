  //Main method that updates everything in game
  void update() {
    if (!gameOver) {
      drawLevel();
      if (!generateLevel) { //Check if level has been generated
        generateMonster();
        generateGate();
        generateItem();
        generateLevel = true;
      }
      drawObjects();
      level.checkCollision();
      moveMonster();
      moveObjects();
      if (player1 != null) player1.updateMovement();
      if (player2 != null) player2.updateMovement();
      if (player3 != null) player3.updateMovement();
      if (player4 != null) player4.updateMovement();
    }
  }

